{
  "app": {
    "security": {
      "csp": "null"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Enhanced Voice Recorder",
        "width": 800
      }
    ],
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.png"
    ],
    "targets": "all"
  },
  "identifier": "com.serverbooster",
  "plugins": {
    "fs": {
      "scope": [
        "*"
      ]
    },
    "shell": {
      "open": true
    }
  },
  "productName": "enhanced-voice-recorder",
  "version": "0.0.0"
}